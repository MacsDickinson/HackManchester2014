@{
    Layout = null;
}
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>HackManchester 2014</title>
    <script type="text/javascript" src="//cdnjs.cloudflare.com/ajax/libs/jquery/2.1.1/jquery.js"></script>
    <script src='https://api.tiles.mapbox.com/mapbox.js/v2.1.3/mapbox.js'></script>
    <script src='https://api.tiles.mapbox.com/mapbox.js/plugins/arc.js/v0.1.0/arc.js'></script>
    <link href='https://api.tiles.mapbox.com/mapbox.js/v2.1.3/mapbox.css' rel='stylesheet' />
    <link href="//cdnjs.cloudflare.com/ajax/libs/semantic-ui/0.19.3/css/semantic.min.css" rel="stylesheet" />
    <style type="text/css">
        body {
            font-family: 'Helvetica Neue', Helvetica, Arial;
            margin: 0;
            padding: 0;
        }

        #map-wrapper {
            position: relative;
            width: auto;
            margin: 5px;
            height: 500px;
        }

        #map {
            position: absolute;
            top: 0;
            bottom: 0;
            left: 0;
            right: 0;
        }
        .path-start {
               -webkit-transition: stroke-dashoffset 5s ease-in;
               -moz-transition: stroke-dashoffset 5s ease-in;
               -o-transition: stroke-dashoffset 5s ease-in;
               transition: stroke-dashoffset 5s ease-in;
           }
    </style>
</head>
    <body>
        @if (Html.RenderContext.Context.CurrentUser != null)
        {
            <div>
                <span>Logged in as @Html.RenderContext.Context.CurrentUser.UserName</span><span><a href="/Logout">Log Out</a></span>
            </div>
        }
        @RenderBody()
    </body>
</html>
